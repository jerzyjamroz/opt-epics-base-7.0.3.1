# ai sensor record
# DESC, EGU set by device support
record(ai, "$(dev):CU$(unit)3d3V-1")
{
    field(DESC, "")
    field(SCAN, "I/O Intr")
    field(DTYP, "MCHsensor")
    field(INP, "#B$(fruid) C2 N1 @$(link)+sens")
    field(PREC, "1")
    field(SDIS, "$(dev):CU$(unit)3d3V-1P.SEVR")
    field(DISV, "2")
    field(PINI, "YES")
}

