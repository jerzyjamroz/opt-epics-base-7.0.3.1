# Detect the first time the event link
# is online.  Then set master enable.
record(calcout, "$(P)$(R=)$(U=U:)Link-Init-FO_") {
  field(DESC, "Detect initial link up")
  field(INPA, "$(P)$(R=)$(U=U:)Link-Sts")
  field(CALC, "A")
  field(OUT , "$(P)$(R=)$(U=U:)Ena-Sel PP")
  field(OOPT, "Transition To Non-zero")
  field(ODLY, "1.0")
  field(TSEL, "$(P)$(R=)$(U=U:)Link-Sts.TIME")
}

record(ao, "$(P)$(R=)$(U=U:)Link-Clk-SP") {
  field(DTYP, "Obj Prop double")
  field(OUT , "@OBJ=$(OBJ):EVRU, PROP=Clock")
  field(PINI, "YES")
  field(DESC, "Event Link speed")
  field(VAL , "$(FEVT=88.0525)")
  field(EGU , "MHz")
  field(LINR, "LINEAR")
