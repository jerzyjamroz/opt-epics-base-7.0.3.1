  field(CALC, "A")
  field(OUT , "$(SYS){$(D)}Ena-Sel PP")
  field(OOPT, "Transition To Non-zero")
  field(ODLY, "1.0")
  field(TSEL, "$(SYS){$(D)}Link-Sts.TIME")
}

