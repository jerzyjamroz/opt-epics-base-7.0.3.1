  field(INPB, "$(SYS){$(D)}Link:Clk-I")
  field(TSEL, "$(SYS){$(D)}Link:Clk-I.TIME")
  field(FLNK, "$(SYS){$(D)}Link:ClkPeriod-I")
  info(autosaveFields_pass0, "EGU CALC PREC")
}

