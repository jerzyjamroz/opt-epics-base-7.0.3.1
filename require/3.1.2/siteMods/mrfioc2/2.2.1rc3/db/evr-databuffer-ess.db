# Init subA_ for the databuffer records

record(subArray, "$(P):dbus-u32indx0-subA_") {
  field(DTYP, "Soft Channel")
  field(INP, "$(P):dbus-recv-u32 CPP")
  field(INDX, "0")
  field(NELM, "1") # "2046")
  field(MALM, "1") # "2046")
  field(FTVL, "ULONG")
}
