record(mbbo, "$(P)$(R=)$(S=:)PBState-SP") {
    field(DTYP, "ControlMbbo")
    field(DESC, "Proton Beam State")
    field(PINI, "YES")
    field(VAL, "Off")
    field(UDF, "Off")
    field(ZRST, "None")
    field(ONST, "Off")
    field(TWST, "On")
    field(THST, "Fault")
    field(FRST, "Recovery")
    field(ZRVL, "0")
    field(ONVL, "10")
