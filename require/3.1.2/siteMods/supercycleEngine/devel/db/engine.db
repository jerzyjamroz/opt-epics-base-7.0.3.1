  field(SELM, "All")

  field(LNK0, "$(P)$(R=)$(S=:)EngineCycle_")
  field(LNK1, "$(P)$(R=)$(S=:)Trig-Cnt_")
  field(LNK2, "$(P)$(R=)$(S=:)Seq1-Tst-aSubBuf_.PROC")
  field(LNK3, "$(P)$(R=)$(S=:)Seq1-Evt-aSubBuf_.PROC")
  field(LNK4, "$(P)$(R=)$(S=:)Seq1-Ctrl-FOut_.PROC")
