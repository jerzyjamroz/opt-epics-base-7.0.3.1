record(longin, "$(P)$(R=)$(S=:)Period") {
  field(DESC, "Actuall Cycle Frequency")
  field(DTYP, "Soft Channel")
  field(SCAN, "Passive")
  field(EGU, "us")
  field(INP, "$(P)$(R=)$(S=:)Period-subA_")

  #field(FLNK, "$(P)$(R=)$(S=:)Seq0-En-Cmd")
  field(HIHI, "91000")
	field(HIGH, "81000")
	field(LOW,  "61000")
	field(LOLO, "51000")
  field(HHSV, "MAJOR")
	field(HSV,  "MINOR")
	field(LSV,  "MINOR")
	field(LLSV, "MAJOR")
}

